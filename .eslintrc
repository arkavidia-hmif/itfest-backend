{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "env": {
    "node": true
  },
  "rules": {
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "semi": [
      "error"
    ],
    "no-var": "error",
    "eqeqeq": "error",
    "no-console": "warn",
    "curly": [
      "error",
      "multi-line"
    ],
    "quotes": "warn"
  },
  "settings": {}
}